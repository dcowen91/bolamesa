{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "trailing-comma": [
      true,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ],
    "no-console": false,
    "no-shadowed-variable": false,
    "no-string-literal": false,
    "object-literal-sort-keys": false,
    "arrow-parens": false
  },
  "rulesDirectory": []
}
